    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.hw9.ui.home.HomeFragment"
        android:label="NewsApp"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="com.example.hw9.ui.bookmark.BookmarkFragment"
        android:label="NewsApp"
        tools:layout="@layout/fragment_bookmark" />

    <fragment
        android:id="@+id/navigation_trend"
        android:name="com.example.hw9.ui.trend.TrendFragment"
        android:label="NewsApp"
        tools:layout="@layout/fragment_trend" />

    <fragment
        android:id="@+id/navigation_section"
        android:name="com.example.hw9.ui.section.SectionFragment"
        android:label="NewsApp"
        tools:layout="@layout/section_fragment" />
